{
  "name": "realtime-chat-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "migrate": "sequelize-cli db:migrate --config sequelize.config.js",
    "seed": "sequelize-cli db:seed:all --config sequelize.config.js",
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "mysql2": "^3.15.2",
    "sequelize": "^6.37.7",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.12",
    "@types/sequelize": "^4.28.20",
    "sequelize-cli": "^6.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
